<%= form_tag({action: :select}) do %>
  <% start_date = session[:created_at] %>
  <% end_date = Date.today %>
  start date:<%= select_date start_date, use_month_numbers: true,
  start_year: start_date.year, end_year: end_date.year, prefix: :start_date %>
  <br />
  end date:<%= select_date end_date, use_month_numbers: true,
  start_year: start_date.year, end_year: end_date.year, prefix: :end_date %>
  <br />
  <%= submit_tag "submit" %>
<% end %>

<%= session[:screen_name] %>
<%= session[:created_at] %>
